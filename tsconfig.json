{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "bundler",
    "noUncheckedIndexedAccess": true,
    "paths": {
      "common-helpers/*": ["./*"]
    },
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext"
  }
}
